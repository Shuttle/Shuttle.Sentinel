<sentinel-form title="datastore:add.title">
  <form>
    <fieldset class="form-group">
      <sentinel-datastore-properties {(name)}="properties.name" {(connection-string)}="properties.connectionString" {(provider-name)}="properties.providerName" />
      <sentinel-submit-button {click}="@add" text="add" />
      <sentinel-button {click}="@close" text="close" />
    </fieldset>
  </form>
</sentinel-form>
