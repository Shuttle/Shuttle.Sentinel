<sentinel-page-title title="subscription:list.title" />
<sentinel-buttons>
  <sentinel-button {click}="@add" text="add" permission="sentinel://subscription/add" />
  <sentinel-refresh-button {click}="@refresh" />
</sentinel-buttons>
{{#if isPending}}
<sentinel-fetching title="queue:list.title" />
{{/if}}
{{#if isResolved}}
<sentinel-table {columns}="columns" {rows}="list" {row-click}="@rowClick" />
{{/if}}