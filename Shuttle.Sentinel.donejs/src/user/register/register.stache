<div class="row">
  <div class="col-md-6 bd-content col-md-offset-3">
    <sentinel-form title="user:register.title">
      <form>
        <fieldset class="form-group">
          <sentinel-text focus="true" label="user:register.email" {(value)}="email" />
          <sentinel-text label="user:register.password" {(value)}="password" type="password" validation-message="{{passwordValidationMessage}}" />
          <sentinel-button ($click)="submit" text="text.register" {icon-name}="submitIconName" {disabled}="working"/>
        </fieldset>
      </form>
    </sentinel-form>
  </div>
</div>
