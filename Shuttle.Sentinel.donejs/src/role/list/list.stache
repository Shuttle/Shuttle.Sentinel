<sentinel-page-title title="role:list.title" />
<sentinel-buttons>
  <sentinel-button (click)="add()" text="add" permission="sentinel://role/add" />
  <sentinel-button (click)="refresh()" text="refresh" icon-name="glyphicon-refresh" />
</sentinel-buttons>
{{#if isPending}}
<sentinel-fetching title="role:list.title" />
{{/if}}
{{#if isResolved}}
<sentinel-table {columns}="columns" {rows}="list" />
{{/if}}