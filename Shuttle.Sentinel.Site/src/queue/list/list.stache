<sentinel-page-title title="queue:list.title" />
<sentinel-buttons>
  <sentinel-button {click}="@add" text="add" permission="sentinel://queue/add" />
  <sentinel-refresh-button {click}="@refresh" />
</sentinel-buttons>
{{#if queuesPromise.isResolved}}
<sentinel-table {columns}="columns" {rows}="queuesPromise.value" />
{{else}}
<sentinel-fetching title="queue:list.title" />
{{/if}}