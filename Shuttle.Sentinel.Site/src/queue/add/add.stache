<cs-form title="queue:add.title">
  <fieldset class="form-group">
    <cs-text focus="true" label="uri" {(value)}="uri" required="true" {errors}="errors" error-attribute="uri"/>
    <cs-button-submit {click}="@add" text="add" />
    <cs-button {click}="@close" text="close" />
  </fieldset>
</cs-form>
