<cs-form title="subscription:add.title">
  <fieldset class="form-group">
    <cs-select label="subscription:data-store" {options}="dataStores" {(value)}="dataStoreId" />
    <cs-text focus="true" label="subscription:message-type" {(value)}="messageType" required="true" {errors}="errors" error-attribute="messageType"/>
    <cs-text focus="true" label="subscription:inbox-work-queue-uri" {(value)}="inboxWorkQueueUri" required="true" {errors}="errors" error-attribute="inboxWorkQueueUri"/>
    <cs-button-submit {click}="@add" text="add" />
    <cs-button {click}="@close" text="close" />
  </fieldset>
</cs-form>
