{
  "name": "sentinel",
  "version": "1.0.0",
  "description": "A management and monitoring solution for Shuttle.Esb implementations.",
  "steal": {
    "main": "main.js",
    "directories": {
      "lib": "src"
    },
    "npmAlgorithm": "flat",
    "plugins": [
      "steal-stache",
      "steal-css"
    ],
    "meta": {
      "popper.js": {
        "format": "global",
        "deps": [
          "jquery"
        ]
      },
      "bootstrap": {
        "deps": [
          "jquery",
          "popper.js"
        ]
      }
    },
    "map": {
      "popper.js": "popper.js/dist/umd/popper",
      "bootstrap": "bootstrap/dist/js/bootstrap"
    },
    "serviceBaseURL": "http://localhost:4040/api/",
    "accessBaseURL": "http://localhost:5599/api/",
    "debug": true,
    "localeVersion": "1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuttle/Shuttle.Sentinel.git"
  },
  "author": "Eben Roux",
  "bugs": {
    "url": "https://github.com/Shuttle/Shuttle.Sentinel/issues"
  },
  "homepage": "https://github.com/Shuttle/Shuttle.Sentinel#readme",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "can-component": "^4.0.7",
    "can-compute": "^4.0.3",
    "can-connect": "^2.0.4",
    "can-define": "^2.0.4",
    "can-define-validate-validatejs": "^1.0.1",
    "can-route": "^4.1.1",
    "can-route-pushstate": "^4.0.4",
    "can-set": "^1.5.2",
    "can-stache": "^4.3.0",
    "can-stache-bindings": "^4.1.3",
    "can-stache-route-helpers": "^0.1.4",
    "can-util": "^3.11.5",
    "can-validate-validatejs": "^0.1.3",
    "i18next": "^11.1.1",
    "i18next-xhr-backend": "^1.5.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "shuttle-access": "^1.0.0",
    "shuttle-can-api": "^1.0.16",
    "shuttle-canstrap": "^1.0.47",
    "shuttle-guard": "^1.0.0",
    "steal": "^1.11.4",
    "steal-css": "^1.3.2",
    "steal-stache": "^4.1.0"
  }
}
