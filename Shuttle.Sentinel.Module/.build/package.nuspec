<?xml version="1.0"?>
<package>
  <metadata>
    <id>Shuttle.Sentinel.Module</id>
    <version>{semver}</version>
    <authors>Eben Roux</authors>
    <owners>Eben Roux</owners>
    <license type="expression">BSD-3-Clause</license>
    <projectUrl>https://github.com/shuttle/Shuttle.Sentinel.Module</projectUrl>
    <icon>images\logo.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>The Shuttle.Sentinel module may be included in a Shuttle.Esb endpoint to gather statistics about the endpoint.</description>
    <releaseNotes />
    <copyright>Copyright (c) {year}, Eben Roux</copyright>
    <tags>shuttle sentinel</tags>
    <dependencies>
      <dependency id="Shuttle.Esb" version="{Shuttle.Esb-version}" />
      <dependency id="Shuttle.Sentinel.Messages" version="{Shuttle.Sentinel.Messages-version}" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\..\..\.media\logo.png" target="images\" />
	<file src="lib\**\*.*" target="lib" />
  </files>
</package>